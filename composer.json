{
    "name": "jagilpe/menu-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony Bundle to build Bootstrap based menus for Web Apps",
    "keywords": ["Symfony Menu"],
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Gil",
            "email": "javier.gil@module-7.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.3",
        "symfony/twig-bundle": ">=2.3",
        "doctrine/collections": "^1.0"
    },
    "require-dev": {
        "symfony/yaml": ">=2.3",
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": { "Jagilpe\\MenuBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
